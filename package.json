{
  "name": "react-spinners-quq99",
  "version": "1.0.0",
  "description": "CSS-only spinners for react",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add",
      "eslint --cache --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quq99/react-spinners-quq99.git"
  },
  "keywords": [
    "react",
    "CSS",
    "spinners"
  ],
  "author": "quq99",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quq99/react-spinners-quq99/issues"
  },
  "homepage": "https://github.com/quq99/react-spinners-quq99#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
